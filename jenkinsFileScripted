node('built-in'){
  
  stage('clone from repository'){
     checkout scm
    }  
    
  stage('Branch-ul main') {
        if (env.BRANCH_NAME == "main") {
            echo 'EXECUTAM DOAR PE BRANCHUL MAIN'
        } else {
            sh "echo branchul este ${env.BRANCH_NAME}"
        }
    }  
    
  stage('git branch'){
      sh "echo asta este ???????? ${env.BRANCH_NAME}"
   }
 
  
  stage('Show env vars') {
    sh "echo ${env.WORKSPACE}"
  }
  
  stage('JAVA') {
    sh 'java -version'
  }
  
  stage('Execute script')
  {
    sh 'ls' 
    sh 'chmod +x HelloWorld.sh'
    sh './HelloWorld.sh'
  }
  
  stage('ORICINE ORICE'){
    sh 'pwd'
  }
  
  stage('Clean up')
  {
    cleanWs deleteDirs: true
  }
}
