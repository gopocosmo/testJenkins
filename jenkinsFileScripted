node('built-in'){
  stage('clone from repository'){
    when { anyOf { 
      branch 'master'; branch 'staging' } }
        checkout scm
  }
  
  stage('Show env vars') {
    sh "echo ${env.WORKSPACE}"
  }
  
  stage('JAVA') {
    sh 'java -version'
  }
  
  stage('Execute script')
  {
    sh 'ls' 
    sh 'chmod +x HelloWorld.sh'
    sh './HelloWorld.sh'
  }
  
  stage('ORICINE ORICE'){
    sh 'pwd'
  }
  
  stage('Clean up')
  {
    cleanWs deleteDirs: true
  }
}
